<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td><%= post.modified %></td>
        <td><%= post.status %></td>
        <td><%= post.title["rendered"] %></td>
        <td><%= post.excerpt["rendered"] %></td>
        <td><%= link_to 'Show', post %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
